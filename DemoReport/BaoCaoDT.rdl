  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select o.OrderID, OrderDate, Quantity, ProductName, TotalPrice  from Products p, Orders o, OrderDetails od
where o.OrderID = od.OrderId and p.ProductID = od. ProductID
and OrderDate between @FromDate and @ToDate</CommandText>
      </Query>
