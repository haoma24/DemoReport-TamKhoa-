  <Code>Public Function NumberToVietnameseText(ByVal number As Decimal) As String
    Dim units() As String = {"", "nghìn", "triệu", "tỷ"}
    Dim digits() As String = {"không", "một", "hai", "ba", "bốn", "năm", "sáu", "bảy", "tám", "chín"}

    If number = 0 Then
        Return "không đồng"
    End If

    Dim result As String = ""
    Dim position As Integer = 0
    Dim groupCount As Integer = 0

    While number &gt; 0
        Dim groupText As String = ""
        Dim group As Integer = number Mod 1000
        number = number \ 1000

        If group &gt; 0 Or groupCount = 0 Then
            Dim hundreds As Integer = group \ 100
            Dim tens As Integer = (group Mod 100) \ 10
            Dim ones As Integer = group Mod 10

            If hundreds &gt; 0 Then
                groupText = digits(hundreds) &amp; " trăm"
                If tens = 0 AndAlso ones &gt; 0 Then
                    groupText &amp;= " linh"
                End If
            End If

            If tens &gt; 0 Then
                If tens = 1 Then
                    groupText &amp;= " mười"
                Else
                    groupText &amp;= " " &amp; digits(tens) &amp; " mươi"
                End If
                If ones &gt; 0 Then
                    If ones = 1 Then
                        groupText &amp;= " một"
                    ElseIf ones = 5 Then
                        groupText &amp;= " lăm"
                    Else
                        groupText &amp;= " " &amp; digits(ones)
                    End If
                End If
            ElseIf ones &gt; 0 Then
                groupText &amp;= " " &amp; digits(ones)
            End If

            If groupText &lt;&gt; "" Then
                result = groupText.Trim() &amp; " " &amp; units(groupCount) &amp; " " &amp; result
            End If
        End If

        groupCount += 1
    End While

    result = result.Trim() &amp; " đồng"
    Return result
End Function</Code>
