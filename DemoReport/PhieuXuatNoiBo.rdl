        </Field>
        <Field Name="Địa_chỉ">
          <DataField>Địa chỉ</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Ngày_lập">
          <DataField>Ngày lập</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Số">
          <DataField>Số</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="PhieuXuatCT">
      <Query>
        <DataSourceName>Boro_eAccounting_Web</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PhieuXuatKhoId">
            <Value>=Parameters!PhieuXuatKhoId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT KhoUd as N'Mã kho', v.VatTuNm as N'Tên vật tư',v.DonViTinh as N'Đơn vị tính', tk2.TaiKhoanUd as N'Tk nợ',
tk.TaiKhoanUd as N'Tk có', ct.SoLuong as N'Số lượng', ct.GiaVND as N'Giá',
ct.TienVND as N'Thành tiền'
FROM PhieuXuatKhoCt ct JOIN Kho k ON ct.KhoId = k.Id JOIN 
TaiKhoan tk ON ct.GhiCoTk = tk.Id JOIN VatTu v ON ct.VatTuId = v.Id
JOIN TaiKhoan tk2 ON ct.GhiNoTk = tk2.Id
WHERE ct.PhieuXuatKhoId = @PhieuXuatKhoId</CommandText>
      </Query>
