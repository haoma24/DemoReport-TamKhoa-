  <DataSets>
    <DataSet Name="PhieuXuat">
      <Query>
        <DataSourceName>Boro_eAccounting_Web</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PhieuXuatKhoId">
            <Value>=Parameters!PhieuXuatKhoId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT NgNhanHang as N'Người nhận hàng',kh.KhachHangNm as N'Đơn vị', kh.DiaChi as N'Địa chỉ',
NgayLap as N'Ngày lập', p.Id as N'Số'
FROM PhieuXuatKho p,  KhachHang kh
WHERE p.KhachHangId = kh.Id and p.Id = @PhieuXuatKhoId</CommandText>
      </Query>
      <Fields>
        <Field Name="Người_nhận_hàng">
          <DataField>Người nhận hàng</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Đơn_vị">
          <DataField>Đơn vị</DataField>
