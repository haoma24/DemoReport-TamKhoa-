    <DataSet Name="PhieuThuCt">
      <Query>
        <DataSourceName>Web_V1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PhieuThuId">
            <Value>=Parameters!PhieuThuId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TaiKhoanUd as N'Có', Sum(ThanhToanVND) as N'Tiền có'
FROM PhieuThuCT ct, TaiKhoan tk
WHERE ct.GhiCoTk = tk.Id and ct.PhieuThuId = @PhieuThuId
GROUP BY TaiKhoanud</CommandText>
      </Query>
      <Fields>
        <Field Name="Có">
          <DataField>Có</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Tiền_có">
          <DataField>Tiền có</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
