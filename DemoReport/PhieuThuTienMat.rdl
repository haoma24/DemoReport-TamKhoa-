  <DataSets>
    <DataSet Name="PhieuThu">
      <Query>
        <DataSourceName>Web_V1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PhieuThuId">
            <Value>=Parameters!PhieuThuId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT pt.Id, pt.NgayLap as 'Ngay', QuyenSoId, TongTienVND as 'Tongtien', SoPhieu, pt.DiaChi, TaiKhoanUd as 'No', NguoiNop
FROM PhieuThu pt,  TaiKhoan tk
WHERE tk.Id = pt.GhiNoTkId and pt.Id = @PhieuThuId</CommandText>
      </Query>
