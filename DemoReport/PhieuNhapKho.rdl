    <DataSet Name="PhieuNhapCt">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PhieuNhapId">
            <Value>=Parameters!PhieuNhapId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT KhoNm as N'Tên kho',v.VatTuUd as N'Mã số', v.VatTuNm as N'Tên vật tư', v.DonViTinh as N'Đơn vị tính',
ct.SoLuong as N'Theo CT',ct.SoLuongConLai as N'Thực nhập', ct.GiaVND as N'Giá', tk.TaiKhoanUd as N'Nợ',
ct.TienVND as N'Thành tiền'
FROM PhieuNhapCt ct JOIN Kho k ON ct.KhoId = k.Id  JOIN VatTu v ON ct.VatTuId = v.Id
JOIN TaiKhoan tk ON tk.Id = ct.GhiNoTK
WHERE PhieuNhapId = @PhieuNhapId</CommandText>
      </Query>
