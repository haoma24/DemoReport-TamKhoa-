  <DataSets>
    <DataSet Name="PhieuNhap">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PhieuNhapId">
            <Value>=Parameters!PhieuNhapId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT NguoiGiaoHang as N'Người giao hàng',kh.KhachHangNm as N'Đơn vị', kh.DiaChi as N'Địa chỉ',
NgayLap as N'Ngày lập', tk.TaiKhoanUd as N'Tk có', p.SoHd as N'Số hóa đơn', soPn as N'Số', p.ChiPhiVND as N'Chi phí', p.TongTienVND as N'Tổng tiền',
p.SoSeri as N'Seri', p.NgayHd as N'Ngày hóa đơn', p.DienGiai as N'Nội dung', p.ThueVatVND as N'Thuế giá trị gia tăng'
FROM PhieuNhap p,  KhachHang kh, TaiKhoan tk
WHERE p.KhachHangId = kh.Id and p.GhiCoTk = tk.Id
and p.Id = @PhieuNhapId</CommandText>
      </Query>
