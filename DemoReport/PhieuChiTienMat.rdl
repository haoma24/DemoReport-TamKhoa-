    </DataSet>
    <DataSet Name="PhieuChiCt">
      <Query>
        <DataSourceName>Boro_Web</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PhieuChiId">
            <Value>=Parameters!PhieuChiId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT TaiKhoanUd as N'Nợ', Sum(ThanhToanVND) 'Tiền ghi nợ'
FROM PhieuChiCT ct, TaiKhoan tk
WHERE ct.GhiNoTk = tk.Id and PhieuChiId = @PhieuChiId
GROUP BY TaiKhoanUd</CommandText>
      </Query>
      <Fields>
        <Field Name="Nợ">
          <DataField>Nợ</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Tiền_ghi_nợ">
          <DataField>Tiền ghi nợ</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
