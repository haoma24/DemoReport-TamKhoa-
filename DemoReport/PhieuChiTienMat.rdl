  <DataSets>
    <DataSet Name="PhieuChi">
      <Query>
        <DataSourceName>Boro_Web</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@PhieuChiId">
            <Value>=Parameters!PhieuChiId.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT pc.NgayLap as 'Ngày', QuyenSoId, TongTienVND as 'Tổng tiền', SoPhieu, pc.DiaChi,
TaiKhoanUd as N'Có', NguoiNhan
FROM PhieuChi pc, TaiKhoan tk
WHERE tk.Id = pc.GhiNoTkId and pc.Id = @PhieuChiId</CommandText>
      </Query>
      <Fields>
        <Field Name="Ngày">
          <DataField>Ngày</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="QuyenSoId">
          <DataField>QuyenSoId</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Tổng_tiền">
          <DataField>Tổng tiền</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="SoPhieu">
