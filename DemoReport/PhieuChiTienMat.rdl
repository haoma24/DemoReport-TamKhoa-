          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DiaChi">
          <DataField>DiaChi</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Có">
