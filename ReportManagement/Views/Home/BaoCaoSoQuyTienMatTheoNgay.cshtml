
@{
    ViewBag.Title = "BaoCao";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<main class="card">
    <div class="card-body justify-content-center align-content-center h-auto overflow-hidden">
        <div class="mt-2 mb-2"><a href="@Url.Action("SoQuyTienMatTheoNgay","Home")" class="btn btn-outline-dark">Quay lại</a></div>
        <section class="h-auto overflow-hidden" aria-labelledby="gettingStartedTitle">
            @{ string name = ViewBag.Name;
                string tungay = ViewBag.FromDate;
                string denngay = ViewBag.ToDate;
                string taikhoan_id = ViewBag.Taikhoan_id;
                string id = ViewBag.Id;
                string link = "";
                if (name == null)
                {
                    <span></span>
                }
                else
                {
                    link = "http://localhost/ReportServer/Pages/ReportViewer.aspx?%2fDemoReport%2f" + name + "&tungay=" + tungay + "&denngay=" + denngay + "&taikhoan_id=" + taikhoan_id + "&rs:Command=Render&rs:Embed=true";
                }



            }
            <iframe style="height: 65vw; border: none; width:100%" src="@link"></iframe>
        </section>
    </div>
</main>

